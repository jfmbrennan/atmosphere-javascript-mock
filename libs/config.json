{
  "port": 8888,
  "url": "/broadcast",
  "logLevel": "off",
  "transport": "long-polling",
  "enableXDR": true,
  "trackMessageLength": true,
  "pollTimeout": 60000,
  "heartbeatPadding": "X",
  "clientHeartbeatInterval": 10000,
  "serverHeartbeatInterval": 55000,
  "messageDelimiter": "|",
  "attachHeadersAsQueryString": [],
  "dropHeaders": [],
  "ssl": false,
  "sslPort": 8443,
  "sslKey": "ssl/key.pem",
  "sslCert": "ssl/cert.pem"
}
